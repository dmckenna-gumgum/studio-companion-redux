{
  "name": "studio-companion-redux",
  "id": "studio-companion-redux",
  "version": "2.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "featureFlags": {
    "enableSWCSupport": true
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "mainPanel",
      "icons": [
        { 
          "width": 23, 
          "height": 23, 
          "path": "icons/pluginIcon.png", 
          "scale": [ 1, 2 ],
          "theme": [ "dark", "darkest" ], 
          "species": [ "generic" ] 
        },
        { 
          "width": 23, 
          "height": 23, 
          "path": "icons/pluginIcon.png", 
          "scale": [ 1, 2 ],
          "theme": [ "lightest", "light" ], 
          "species": [ "generic" ] 
        }  
      ],
      "label": {
        "default": "Studio Companion Redux"
      },
      "minimumSize": {
        "width": 300,
        "height": 300
      },
      "maximumSize": {
        "width": 6000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 1200,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 1200,
        "height": 300
      }
    }
  ],
  "icons": [
    { 
      "width": 23, 
      "height": 23, 
      "path": "icons/pluginIcon.png", 
      "scale": [ 1, 2 ],
      "theme": [ "dark", "darkest" ], 
      "species": [ "generic" ] 
    },
    { 
      "width": 23, 
      "height": 23, 
      "path": "icons/pluginIcon.png", 
      "scale": [ 1, 2 ],
      "theme": [ "lightest", "light" ], 
      "species": [ "generic" ] 
    }  
  ],  
  "requiredPermissions": {
    "localFileSystem": "request",
    "network": {
        "domains": ["https://*"]
    }
  },
  "host": [
    {
      "app": "PS",
      "minVersion": "24.4.0"
    }
  ]
}
